{
  "name": "create-react-app-and-cordova",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "cordova-browser": "^5.0.3",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "npm run remove:www && react-scripts start",
    "build": "npm run remove:www && react-scripts build",
    "build:cordova": "npm run remove:www && react-scripts build && node scripts/www.js && mv build www",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "remove:www": "rm -rf www"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "ios"
    ]
  }
}